{
  "name": "nativescript-bottombar",
  "version": "3.0.8",
  "description": "NativeScript plugin for AHBottomNavigation.",
  "main": "bottombar",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "3.2.0",
      "ios": "3.2.0"
    },
    "plugin": {
      "nan": "true",
      "pan": "true",
      "core3": "true",
      "vue": "true",
      "webpack": "false",
      "category": "Developer"
    }
  },
  "scripts": {
    "build": "tsc",
    "demo.ios": "npm run preparedemo && cd ../demo-ng && tns run ios",
    "demo.android": "npm run preparedemo && cd ../demo-ng && tns run android",
    "preparedemo": "npm run build && cd ../demo-ng && tns plugin remove nativescript-bottombar && tns plugin add ../src && tns install",
    "setup": "cd ../demo-ng && npm install && cd ../src && npm run build && cd ../demo-ng && tns plugin add ../src && cd ../src",
    "typingz": "cd ../demo-ng && TNS_TYPESCRIPT_DECLARATIONS_PATH='$(pwd)/typingz' tns build ios"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rhanbIT/nativescript-bottombar.git"
  },
  "plugin": {
    "nan": "true",
    "pan": "true",
    "core3": "true",
    "category": "Interface"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "TypeScript",
    "Angular",
    "BottomBar",
    "Bottom bar",
    "Material Design",
    "bottom navigation",
    "tab navigation",
    "Android",
    "iOS",
    "MiniTabBar"
  ],
  "author": {
    "name": "rhanbIT",
    "email": "aniel.jeanbaptiste@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/rhanbIT/nativescript-bottombar/issues"
  },
  "homepage": "https://github.com/rhanbIT/nativescript-bottombar",
  "readmeFilename": "README.md",
  "devDependencies": {
    "tns-core-modules": "^3.2.0",
    "tns-platform-declarations": "^3.2.0",
    "typescript": "^2.3.2"
  }
}
